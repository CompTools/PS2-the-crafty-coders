3.cat flights.May2017-Apr2018.csv | grep -i "***" | cut -d "," -f 3 | sort -u >> airportcodes.txt 

4.cat flights.May2017-Apr2018.csv | cut -d "," -f 4,5 | grep '\w, FL' | sort -u >> floridacities.txt
