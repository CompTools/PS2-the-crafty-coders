cat flights.May2017-Apr2018.csv | cut -d "," -f 3,7,13,16,24 | grep '"GNV","ATL"' | wc -l >> GNVtoATLtotal.txt Answer: 1476
cat flights.May2017-Apr2018.csv | cut -d "," -f 3,7,13,16,24 | grep '"GNV","CLT"' | wc -l >> GNVtoCLTtotal.txt Answer: 476
